# ENSHunter Configuration
# Copy this file to .env and update with your settings

# Required: Your Infura API key
INFURA_KEY=your_infura_project_id_here

# Optional: Number of concurrent workers (default: 5)
WORKERS=10

# Optional: Rate limit in milliseconds between requests (default: 100)
RATE_LIMIT=50

# Optional: Number of retries for failed requests (default: 3)
RETRIES=3

# Optional: Request timeout in seconds (default: 30)
TIMEOUT=30
